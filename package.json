    "@sanity/types": "^3.65.0",
    "@sanity/ui": "^2.8.8",
    "@sanity/uuid": "^3.0.2",
    "@sanity/vision": "^3.65.0",
    "@sanity/webhook": "^4.0.4",
    "axios": "^1.5.0",
    "concurrent-queue": "^7.0.2",
    "date-fns": "^2.30.0",
    "dotenv": "^8.2.0",
    "easymde": "^2.18.0",
    "emoji-mart": "^3.0.0",
    "google-auth-library": "^8.7.0",
