{
  "name": "sanity-community-studio-and-bot",
  "private": true,
  "version": "1.0.0",
  "description": "Sanity Community Studio and Slack Bot",
  "main": "package.json",
  "author": "Knut Melv√¶r <knut@sanity.io>",
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "proseWrap": "always",
    "quoteProps": "consistent",
    "singleQuote": true
  },
  "license": "MIT",
  "scripts": {
    "_dev": "ts-node-dev src/dev.ts --respawn --transpileOnly",
    "dev": "sanity dev",
    "start": "sanity start",
    "build": "sanity build",
    "deploy": "sanity deploy",
    "deploy-graphql": "sanity graphql deploy",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint '**/*.{js,jsx,mjs,ts,tsx}' --quiet",
    "lint:felino": "felino",
    "lint:prettier": "prettier . --check --ignore-unknown",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint '**/*.{js,jsx,mjs,ts,tsx}' --quiet --fix",
    "fix:prettier": "prettier . --write --ignore-unknown"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@google-cloud/pubsub": "^3.2.1",
