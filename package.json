    "easymde": "^2.18.0",
    "emoji-mart": "^3.0.0",
    "google-auth-library": "^8.7.0",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.13",
    "p-queue": "^7.4.1",
    "p-retry": "^6.0.0",
