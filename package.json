    "p-retry": "^6.0.0",
    "prop-types": "^15.8",
    "qs": "^6.9.4",
    "react": "^18.3.1",
    "react-compiler-runtime": "19.0.0-beta-df7b47d-20241124",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.3.1",
    "react-is": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-player": "^2.13.0",
    "rxjs": "^7.8.1",
    "sanity": "^3.65.0",
    "sanity-mobile-preview": "^1.0.9",
    "sanity-plugin-markdown": "^5.0.0",
