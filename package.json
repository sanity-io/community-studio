{
  "name": "sanity-community-studio-and-bot",
  "private": true,
  "version": "1.0.0",
  "description": "Sanity Community Studio and Slack Bot",
  "main": "package.json",
  "author": "Knut Melv√¶r <knut@sanity.io>",
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "proseWrap": "always",
    "quoteProps": "consistent",
    "singleQuote": true
  },
  "license": "MIT",
  "scripts": {
    "_dev": "ts-node-dev src/dev.ts --respawn --transpileOnly",
    "dev": "sanity dev",
    "start": "sanity start",
    "build": "sanity build -y && sanity schema extract --path ./public/schema.json",
    "deploy": "sanity deploy",
    "deploy-graphql": "sanity graphql deploy",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint '**/*.{js,jsx,mjs,ts,tsx}' --quiet",
    "lint:felino": "felino",
    "lint:prettier": "prettier . --check --ignore-unknown",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint '**/*.{js,jsx,mjs,ts,tsx}' --quiet --fix",
    "fix:prettier": "prettier . --write --ignore-unknown"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@google-cloud/pubsub": "^3.7.5",
    "@portabletext/react": "^3.2.1",
    "@portabletext/types": "^2.0.13",
    "@sanity/block-content-to-hyperscript": "^3.0.0",
    "@sanity/block-content-to-markdown": "^0.0.6",
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/cli": "^4.1.1",
    "@sanity/client": "^6.29.1",
    "@sanity/code-input": "^5.1.3",
    "@sanity/color-input": "^4.0.5",
    "@sanity/embeddings-index-ui": "^2.1.1",
    "@sanity/icons": "^3.7.4",
    "@sanity/image-url": "^1.1.0",
    "@sanity/types": "^4.1.1",
    "@sanity/ui": "^3.0.1",
    "@sanity/uuid": "^3.0.2",
    "@sanity/vision": "^4.3.0",
    "@sanity/webhook": "^4.0.4",
    "axios": "^1.10.0",
    "concurrent-queue": "^7.0.2",
    "date-fns": "^2.30.0",
    "dotenv": "^8.6.0",
    "easymde": "^2.20.0",
    "emoji-mart": "^3.0.1",
    "google-auth-library": "^8.9.0",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.11",
    "openai": "^4.104.0",
    "p-queue": "^7.4.1",
    "p-retry": "^6.2.1",
    "prop-types": "^15.8.1",
    "qs": "^6.14.0",
    "react": "^19.2.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^19.2.1",
    "react-is": "^19.2.1",
    "react-markdown": "^9.1.0",
    "react-player": "^2.16.0",
    "rxjs": "^7.8.2",
    "sanity": "^4.3.0",
    "sanity-mobile-preview": "^1.0.9",
    "sanity-plugin-markdown": "^5.1.2",
    "sanity-plugin-utils": "^1.6.8",
    "semver": "^7.7.2",
    "speakingurl": "^14.0.1",
    "styled-components": "^6.1.19",
    "validate-npm-package-name": "^4.0.0"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^5.0.2",
    "@types/dotenv": "^8.2.3",
    "@types/emoji-mart": "^3.0.14",
    "@types/lodash": "^4.17.19",
    "@types/node": "^20.19.1",
    "@types/qs": "^6.14.0",
    "@types/semver": "^7.7.0",
    "@types/validate-npm-package-name": "^4.0.2",
    "@vercel/node": "^3.2.29",
    "babel-plugin-react-compiler": "rc",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-path-alias": "^1.1.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react-compiler": "rc",
    "eslint-plugin-react-hooks": "^5.2.0",
    "npm-run-all2": "^5.0.2",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@9.14.2",
  "pnpm": {
    "overrides": {
      "@sanity/cli": "$@sanity/cli",
      "@sanity/types": "$@sanity/types",
      "@sanity/vision": "$@sanity/vision",
      "eslint-plugin-react-hooks": "$eslint-plugin-react-hooks",
      "sanity": "$sanity"
    }
  }
}
