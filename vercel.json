{
  "version": 2,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/auth/login",
      "dest": "/static/auth/login.html"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
  "env": {
    "SANITY_PROJECT_ID": "@community-bot-sanity-project-id",
    "SANITY_DATASET": "@community-bot-sanity-dataset",
    "SANITY_WRITE_TOKEN": "@community-bot-sanity-write-token",
    "SANITY_CREATE_SESSION_TOKEN": "@community-bot-sanity-create-session-token",
    "SANITY_STUDIO_URL": "@community-bot-sanity-studio-url",
    "GOOGLE_OAUTH_CLIENT_ID": "@community-bot-google-oauth-id",
    "GOOGLE_OAUTH_CLIENT_SECRET": "@community-bot-google-oauth-secret",
    "SLACK_BOT_USER_TOKEN": "@community-bot-slack-bot-user-token",
    "EMAIL_DOMAIN": "@community-bot-email-domain",
    "SLACK_TOKEN_A": "@sanity-slack-token-a",
    "SLACK_TOKEN_B": "@sanity-slack-token-b"
  }
}
